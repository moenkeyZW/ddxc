/* pages/music/index.wxss */
@import '../templet/index.wxss';
.byoMusic{
  width:calc(100% - 60rpx);
  height:100rpx;
  display: flex;
  justify-content: space-between;
  background:#fff;
  color:#333;
  font-size:32rpx;
  align-items: center;
  margin-bottom:20rpx;
  margin-top:-10rpx;
}
.dw{
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  margin-right:30rpx;
}
.first{
  margin:0 0 0 30rpx;
  border-bottom: 1rpx solid #ebebeb;
  align-items: center;
}
.broadcast{
  width:48rpx;
  height:48rpx;
  margin-right:24rpx;
}
.biao{
  padding-top:10rpx;
}
.gm{
  padding:3rpx 0 10rpx 0;
}
.intro{
  width:100%;
  border:none;
  margin-left:0rpx;
}